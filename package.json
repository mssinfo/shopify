{
    "name": "framework",
    "version": "1.0.0",
    "private": true,
    "description": "Project scaffold with webpack and SCSS build",
    "scripts": {
        "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=node_modules/laravel-mix/setup/webpack.config.js --mode development --open",
        "build": "node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js --mode production --progress",
        "build:scss": "sass src/styles:dist/styles --no-source-map --style=compressed",
        "watch:scss": "sass --watch src/styles:dist/styles --style=expanded",
        "clean": "rm -rf dist || rmdir /s /q dist"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "autoprefixer": "^10.4.14",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.8.1",
        "html-webpack-plugin": "^5.5.1",
        "mini-css-extract-plugin": "^2.7.5",
        "postcss-loader": "^7.3.2",
        "sass": "^1.72.0",
        "sass-loader": "^13.3.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "laravel-mix": "^6.0.49"
    }
}
